{
	"name": "rocketh-deploy-proxy",
	"version": "0.6.13",
	"description": "provide proxy-enabled deploy function for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"abitype": "^0.8.11",
		"eip-1193": "^0.4.7",
		"rimraf": "^5.0.1",
		"rocketh": "workspace:*",
		"tsup": "^7.1.0",
		"typescript": "^5.1.3"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"named-logs": "^0.2.2",
		"rocketh-deploy": "workspace:*",
		"solidity-proxy": "^0.2.2",
		"viem": "^1.1.7"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format esm,cjs --watch"
	}
}
