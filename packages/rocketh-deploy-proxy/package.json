{
	"name": "rocketh-deploy-proxy",
	"version": "0.5.10",
	"description": "provide proxy-enabled deploy function for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"abitype": "^0.7.1",
		"eip-1193": "^0.4.7",
		"rimraf": "^4.4.1",
		"rocketh": "workspace:*",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"rocketh-deploy": "workspace:*",
		"solidity-proxy": "^0.0.0",
		"viem": "^0.3.50"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format esm,cjs --watch"
	}
}
